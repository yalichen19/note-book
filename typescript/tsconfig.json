{
  "compilerOptions": {
    "target": "es2016",
    "module":"commonjs",
    "lib": ["es2016", "es2017"],
    "outDir": "dist",
    "strictNullChecks": true,
    "removeComments": true,
    "noImplicitUseStrict": true,
    "esModuleInterop": true,
    "noEmitOnError":true,
    "moduleResolution": "Node",
    "strictPropertyInitialization": true
  },
  "include": [
    "src",
  ],
  "exclude": [
    "src/ex5-ticTacToe"
  ]
}
